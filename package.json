{
  "name": "typed-searcher",
  "version": "1.0.0",
  "homepage": "./",
  "proxy": "http://localhost:6000",
  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && npm install && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "client": "cd client && npm start",
    "start": "nodemon server.ts",
    "dev": "concurrently -n 'server,client' -c 'white,yellow' \"nodemon server.ts\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^6.4.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "enzyme": "^3.11.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tslib": "^1.14.1",
    "typescript": "^4.5.2"
  }
}
